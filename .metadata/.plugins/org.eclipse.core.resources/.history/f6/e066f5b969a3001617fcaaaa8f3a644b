package template;

import java.util.HashMap;
import java.util.LinkedList;

public class CentralizedPlan {
	public HashMap<Integer, LinkedList<CentralizedTask> plan = new HashMap<Integer, LinkedList<CentralizedTask>();
	
	public CentralizedPlan(HashMap<Integer, LinkedList<CentralizedTask> plan) {
		this.plan = plan;
	}
}
